<div class="stock-widget {$TickStatus}">
    <span class="symbol">{$Symbol}</span>
    <span class="last-price">{$LastPrice}</span>
    <span class="previous-close">{$PreviousClose}</span>
    <span class="price-difference">{$PriceDifference}</span>
    <span class="percent-difference">{$PercentDifference.Nice}</span>
    <span class="last-updated">{$LastUpdated.Nice}</span>
    <span class="volume">{$Volume.Formatted}</span>
</div>